{
  "name": "plethem",
  "version": "1.0.0",
  "description": "This application is used to predict the health effects arising from exposure to various chemicals. ",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config ./webpack/webpack.config.js --env.env=dev",
    "build:prod:bundleVisualizer": "npm run build -- --progress --profile --env.addons=bundleVisualizer",
    "build:prod:bundleAnalyzer": "npm run build -- --env.addons=bundleAnalizer",
    "build:prod:stats": "npm run build -- --progress --profile --json > ./dist/stats/stats.json",
    "build": "webpack --config ./webpack/webpack.config.js --env.env=prod",
    "debug:prod": "npm run debug -- --env.env=prod",
    "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./webpack/webpack.config.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruulik/PLETHEM.git"
  },
  "author": "karoliskrulis@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kruulik/PLETHEM/issues"
  },
  "homepage": "https://github.com/kruulik/PLETHEM#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "prop-types": "^15.6.0",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "sass-mediaqueries": "^1.6.1",
    "core-js": "2.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "2.30.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "node-sass": "4.5.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-merge": "4.1.0",
    "webpack-visualizer-plugin": "0.1.11",
    "react-dev-utils": "^4.2.1",
    "react-error-overlay": "^3.0.0"
  }
}
