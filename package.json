{
  "name": "plethem",
  "version": "0.0.1",
  "description": "This application is used to predict the health effects arising from exposure to various chemicals. ",
  "author": "karoliskrulis@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruulik/PLETHEM.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kruulik/PLETHEM/issues"
  },
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "build-dev": "better-npm-run build-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__SERVER__": false
    },
    "moduleFileExtensions": [
      "",
      "json",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build-dev": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "express-http-proxy": "^1.1.0",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^2.3.1",
    "humps": "^2.0.0",
    "invariant": "^2.2.1",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.5.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.15.0",
    "normalizr": "^3.2.4",
    "pretty-error": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^16.2.0",
    "react-ga": "^2.1.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-scroll": "^0.4.1",
    "react-tools": "^0.13.3",
    "react-transform": "0.0.3",
    "redux": "^3.5.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.0",
    "url-loader": "^0.6.2",
    "warning": "^3.0.0",
    "webpack-isomorphic-tools": "^3.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "^0.1.0",
