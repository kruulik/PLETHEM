{
  "name": "plethem",
  "version": "0.0.1",
  "description": "This application is used to predict the health effects arising from exposure to various chemicals. ",
  "author": "karoliskrulis@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruulik/PLETHEM.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kruulik/PLETHEM/issues"
  },
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --env.env=dev",
    "build:prod:bundleVisualizer": "npm run build -- --progress --profile --env.addons=bundleVisualizer",
    "build:prod:bundleAnalyzer": "npm run build -- --env.addons=bundleAnalizer",
    "build:prod:stats": "npm run build -- --progress --profile --json > ./dist/stats/stats.json",
    "build": "webpack --config ./webpack/webpack.config.js --env.env=prod",
    "debug:prod": "npm run debug -- --env.env=prod",
    "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./webpack/webpack.config.js"
  },
  "jest": {
    "globals": {
      "__SERVER__": false
    },
    "moduleFileExtensions": [
      "",
      "json",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./src/plethem.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./src/plethem.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build-dev": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
